<h1>Editing searched_keyword</h1>

<% form_for(@searched_keyword) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @searched_keyword %> |
<%= link_to 'Back', searched_keywords_path %>