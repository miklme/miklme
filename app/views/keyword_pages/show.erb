
<%= render :partial => "resources/form" %>
<div id="results">
  <div id="tiantang"><h2 class="comfort">天堂</h2></div>


  <% for u in @users  %>
    <%= render  :partial => "resources/bubble_resource",:object => u.resources.find(:first,:order => "created_at DESC",:conditions => {:keyword_page_id => @keyword_page.id})  %>
    <ol class="sb_results" id="wg0">
      <% user_id=u.id %>
      <li id="more_of_<%=user_id%>">
        <div class="more"><%= link_to_remote '· · · · · ·',:url => {:action => :more,:user_id => u.id,:keyword_page_id => @keyword_page.id}%></div>
      </li>
    </ol>
  <% end %>
  <div id="diyu">
    <h2 class="highlight">地狱</h2>
  </div>
  <div class="sb_pag">
    <%= will_paginate @users, :prev_label => '上一页', :next_label => '下一页' %>
  </div>
</div>

