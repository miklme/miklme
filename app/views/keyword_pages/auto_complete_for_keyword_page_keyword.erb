<ul class="auto_complete_list">
  <% if @keyword_pages.present? %>
    <% for k in @keyword_pages %>
      <li class="auto_complete_item"><%= k.keyword %></li>
    <% end %>
  <% else %>
    <li class="auto_complete_item">抱歉，暂时没有与<strong><%=params[:keyword_page][:keyword] %></strong>相关的关键字</li>
  <% end %>
</ul>